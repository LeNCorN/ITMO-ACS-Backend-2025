   "type": "commonjs",
   "main": "index.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "ts-node -r reflect-metadata src/index.ts",
      "build": "tsc",
      "dev": "nodemon --exec \"ts-node -r reflect-metadata src/index.ts\"",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:run": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
      "migration:generate": "ts-node --transpile-only node_modules/typeorm/cli.js migration:generate src/migrations/InitMigration -d src/data-source.ts",
      "migration:create": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:create src/migrations/Init",
      "migration:revert": "ts-node --transpile-only node_modules/typeorm/cli.js migration:revert -d src/data-source.ts",
      "swagger:gen": "ts-node src/swagger.ts",
      "prestart": "pnpm swagger:gen"
   },
   "devDependencies": {
      "@types/express": "^5.0.1",
      "@types/node": "^22.14.1",
      "@types/swagger-ui-express": "^4.1.8",
      "nodemon": "^3.1.10",
      "swagger-autogen": "^2.23.7",
      "ts-node": "^10.9.2",
      "tsx": "^4.19.3",
      "typeorm": "^0.3.22",
      "typescript": "^5.8.3"
   },
   "dependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/jsonwebtoken": "^9.0.9",
      "@types/memory-cache": "^0.2.6",
      "bcrypt": "^5.1.1",
      "bcryptjs": "^3.0.2",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.1",
      "class-validator-jsonschema": "^5.0.2",
      "concurrently": "^9.1.2",
      "cors": "^2.8.5",
      "dotenv": "^16.5.0",
      "express": "^5.1.0",
      "jsonwebtoken": "^9.0.2",
      "memory-cache": "^0.2.0",
      "mysql": "^2.18.1",
      "pg": "^8.14.1",
      "reflect-metadata": "^0.2.2",
      "routing-controllers": "^0.11.2",
      "routing-controllers-openapi": "^5.0.0",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.1",
      "ts-morph": "^25.0.1",
      "tsconfig-paths": "^4.2.0"
   }
}
